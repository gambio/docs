{
    "required": [
        "id",
        "orderId",
        "code",
        "isReturnDelivery",
        "parcelService",
        "createdOn"
    ],
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "format": "int32",
            "example": 1,
            "minimum": 1
        },
        "orderId": {
            "type": "integer",
            "format": "int32",
            "example": 1,
            "minimum": 1
        },
        "code": {
            "type": "string",
            "example": "123456789"
        },
        "isReturnDelivery": {
            "type": "boolean",
            "example": true
        },
        "parcelService": {
            "$ref": "./parcelServiceDetails.json"
        },
        "createdOn": {
            "type": "string",
            "pattern": "^\\d{4}-\\d{2}-\\d{2}(\\s\\d{2}:\\d{2}:\\d{2})?$",
            "example": "2020-04-15 14:30:35"
        }
    }
}