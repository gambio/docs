@startuml
'https://plantuml.com/class-diagram

!theme gambio from ../gambio-puml-theme

interface IteratorAggregate

package Gambio\Admin\Modules\Newsletter\Model\Collections {

    class CustomerIds {
        + {static} create(CustomerId ...$customerIds): CustomerIds
        + toArray(): array
    }

}

package Gambio\Admin\Modules\Newsletter\Model\ValueObjects {

    class CustomerId {
        - value: int
        --
        + {static} create(int $value): CustomerId
        ..
        + value(): int
    }
}

CustomerIds o- CustomerId
CustomerIds --|> IteratorAggregate

@enduml